    "def extend_euclid(a,b):\n",
    "    if b == 0:\n",
    "        return a, 1,0\n",
    "    gcd, x1, y1 =  extend_euclid(b, a%b)\n",
    "    x = y1\n",
    "    y = x1 - (a//b)*y1\n",
    "    return gcd, x, y\n"
   ]
  }
